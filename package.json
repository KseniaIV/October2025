{
  "name": "google-news-rss-reader",
  "version": "1.0.0",
  "description": "Static RSS reader that converts Google News RSS feeds to JSON",
  "type": "module",
  "scripts": {
    "fetch-news": "node scripts/rss-to-json.js",
    "dev": "python -m http.server 8000",
    "build": "npm run fetch-news",
    "start": "npm run dev",
    "deploy": "npm run build"
  },
  "dependencies": {
    "rss-parser": "^3.13.0"
  },
  "devDependencies": {},
  "keywords": ["rss", "news", "google-news", "static-site"],
  "author": "",
  "license": "MIT"
}
